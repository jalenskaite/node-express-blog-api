{
  "name": "node-express-blog-api",
  "version": "1.0.0",
  "description": "Simple Node Express blog api",
  "main": "dist",
  "scripts": {
    "start": "node -r esm server/server.js",
    "dev": "nodemon -r esm server/server.js",
    "lint": "eslint server/**/*.js",
    "test": "export NODE_ENV=test || SET \"NODE_ENV=test\" && mocha -r esm server/**/*.test.js",
    "test-watch": "nodemon --exec \"yarn test\""
  },
